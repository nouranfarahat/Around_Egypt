<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.a34ml.experience.view.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_experienceDetailsFragment"
            app:destination="@id/experienceDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/experienceDetailsFragment"
        android:name="com.example.a34ml.experiencedetails.view.ExperienceDetailsFragment"
        android:label="ExperienceDetailsFragment" >
        <argument
            android:name="experience_details"
            app:argType="com.example.a34ml.model.experiencemodel.Experience" />
    </fragment>
</navigation>